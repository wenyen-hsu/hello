import requests
from bs4 import BeautifulSoup

# 台达电的新闻网页URL，注意：这个URL可能会改变，需要根据实际情况调整
url = "https://www.deltaww.com/news/press"

# 发送HTTP请求
response = requests.get(url)

# 解析HTTP响应内容
soup = BeautifulSoup(response.text, 'html.parser')

# 查找新闻标题和链接（根据实际网页结构调整）
# 这个例子假设新闻标题是在<h2>标签内，而且包含链接
for news_item in soup.find_all('h2'):
    link = news_item.find('a')
    if link:
        print(f"标题: {link.text}")
        print(f"链接: {link['href']}")